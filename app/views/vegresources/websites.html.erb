<h1 class="title" style="margin: 0 -.4em;padding: .1em .4em">Websites</h1>


<% @websites.each do |vr| %>
<%=render :partial => '/vegresources/listitem', :locals => {:vr => vr}%>
<% end %>

<br />
<% if can? :create, Vegresource %>
<%= link_to 'Add Website', new_vegresource_path(:vrtype => 'website'),:class => "btn btn-primary" %>
<% end %>
